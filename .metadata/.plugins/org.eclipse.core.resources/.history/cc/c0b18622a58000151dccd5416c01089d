import java.util.Calendar;

public class Cota{
	public String nome;
	public double valPago;
	public Calendar dataVencimento, dataPagamento;
	public String obs;
	
	public Cota(String nome, double valO, double valP, Calendar dVenc, Calendar dPag, String obs){
		this.nome = nome; valPago = valP; dataVencimento = dVenc; dataPagamento = dPag; this.obs = obs;
	}
	
	public  Calendar getDataVencimento() { return dataVencimento; }
	
	public String toString(){
		return "\tData Vencimento" + dataVencimento + 
				"\tValor Pago : " + valPago + "\tData Pagamento : " + dataPagamento +
					"Observação : " + obs;
	}
}
